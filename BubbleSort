import random

list = []

quantidade= int(input("Quantidade: "))
inicio = int(input("Inicio: "))
fim = int(input("Fim: "))

for i in range(quantidade):
    list.append(random.randint(inicio, fim))
    
print("lista padrÃ£o: ",list)

def BubbleSort():
    n = len(list)
    for i in range(n-1):
        for j in range(n-i-1):
            if list[j] > list[j+1]: 
                list[j], list[j+1] = list[j+1], list[j]
    print("BubbleSort: ", list)
        
BubbleSort()
